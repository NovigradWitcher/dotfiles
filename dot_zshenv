#For react native debugging
export BROWSER='Brave Browser'
export REACT_NATIVE_DEBUGGER="open -a 'Brave Browser'"

# Oh My Zsh compdump cache path
export ZSH_COMPDUMP=$ZSH/cache/.zcompdump-$HOST

#For Bitwarden's SSH Agent
export SSH_AUTH_SOCK=~/.var/app/com.bitwarden.desktop/data/.bitwarden-ssh-agent.sock

# For winapps (Local install)
WINAPPS_SRC_DIR="$HOME/.local/bin/winapps-src"

# For android
export ANDROID_HOME="/home/linuxbrew/.linuxbrew/share/android-commandlinetools"
export PATH="$ANDROID_HOME/emulator:$ANDROID_HOME/platform-tools:$PATH"
